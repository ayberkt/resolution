entrypoints FormExp;

PropName.    FormExp5  ::= Ident;
NegExp.      FormExp4  ::= "~" FormExp5;
DisjExp.     FormExp3  ::= FormExp3 "\\/" FormExp4;
ConjExp.     FormExp2  ::= FormExp2 "/\\" FormExp3;
